    android:label="MyApp" 
    android:theme="@style/Theme.AppCompat.Light">
        <activity 
            android:name="com.example.myapp.MainActivity"
            android:exported="true"> 
            <intent-filter>
                <action 
                android:name="android.intent.action.MAIN" /> 
                 <category 
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter> 
           </activity> 
          </application>
</manifest>
